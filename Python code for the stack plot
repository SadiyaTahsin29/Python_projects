import matplotlib.pyplot as plt
import numpy as np

# Generate x values and corresponding y values for sine and cosine
x = np.linspace(0, 10, 100)
y1 = np.sin(x)
y2 = np.cos(x)

# Create a stack plot with a 'wiggle' baseline
plt.stackplot(x, y1, y2, baseline='wiggle')

# Add title to the plot
plt.title('Streamgraph')

# Display the plot
plt.show()
