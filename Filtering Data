import pandas as pd
data = {
  'CustomerID': [1, 2, 3, 4, 5],
  'Name': ['Alice', 'Bob', 'Charlie', 'David', 'Eve'],
  'Age': [17, 22, 15, 35, 40]
}
df = pd.DataFrame(data)

def filter_customers(df):
  filtered_df = df[df['Age'] >= 18]
  return filtered_df
filtered_customers = filter_customers(df)
print(filtered_customers)
